generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  password String
  todos    Todo[]
}

model Todo {
  id     String @id @default(uuid())
  title  String
  user   User?  @relation(fields: [userId], references: [id])
  userId String
}
